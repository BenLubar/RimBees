	<!--========== Tier 2: Specialized Branches ==========-->

	<!--=============== Alloy ====================-->

	<ThingDef ParentName="RB_BeeBase" Name="RB_AlloyBeeBase" Abstract="True" >
		<description>Alloy bees are capable of producing honeycombs with metallic compounds in them. Their honey production is minimal, though. The basic Alloy Bee produces steel plated honeycombs, but it can be crossbred to get more specialized bees. Note that this bee needs a stockpile with some steel nearby to be able to do its job!\n\nBee Traits:\n\nProduction time: 24 hours\nNocturnal: No\nPluviophile: No\nUnusual plants or resource needed: Steel\nMinimum Temperature: 0ºC\nMaximum temperature: 30ºC</description> 
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>   
		<comps>			
			<li Class="RimBees.CompProperties_Bees">
				<species>Alloy</species>
				<comb>RB_Alloy_Honeycomb</comb>
				<combtimedays>1</combtimedays>
				<nocturnal>false</nocturnal>
				<pluviophile>false</pluviophile>
				<weirdplantneeded>Steel</weirdplantneeded>
				<tempMin>0</tempMin>
				<tempMax>30</tempMax>
			</li>   
		</comps>
	</ThingDef>

	<ThingDef ParentName="RB_AlloyBeeBase">
		<defName>RB_Bee_Alloy_Drone</defName>
		<label>alloy drone</label>
		<graphicData>
			<texPath>Things/Item/Bees/RB_Bee_Alloy_Drone</texPath>
		</graphicData> 		
