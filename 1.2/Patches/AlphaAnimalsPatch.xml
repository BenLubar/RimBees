							<graphicData>
								<texPath>Things/Item/Bees/RB_Bee_Devil_Queen</texPath>
							</graphicData> 		
						</ThingDef>

						<!--=============== Feral ====================-->

						<ThingDef ParentName="RB_BeeBase" Name="RB_FeralBeeBase" Abstract="True" >
							<description>Feral bees share some of the best -and worst- characteristics of the Cinderlisk spiders. Their tiny mandibles are always burning-hot, and they can slowly gather cotton fibers from nearby plants and convert them into Cindersilk.\n\nBee Traits:\n\nProduction time: 72 hours\nNocturnal: No\nPluviophile: No\nUnusual plants needed: Cotton plant\nMinimum Temperature: 0ºC\nMaximum temperature: 50ºC</description>
							<graphicData>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>   
							<tradeability>Buyable,Sellable</tradeability>
							<comps>			
								<li Class="RimBees.CompProperties_Bees">
									<species>Feral</species>
									<comb>RB_Feral_Honeycomb</comb>
									<combtimedays>3</combtimedays>
									<nocturnal>false</nocturnal>
									<pluviophile>false</pluviophile>
									<weirdplantneeded>Plant_Cotton</weirdplantneeded>
