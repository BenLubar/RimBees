<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>



				<li Class="PatchOperationAdd"> 
					<xpath>/Defs</xpath>
					<value>

						<!--========== Alpha Animals Branch ==========-->

						<!--=============== Black ====================-->

						<ThingDef ParentName="RB_BeeBase" Name="RB_BlackBeeBase" Abstract="True" >
							<description>Black bees are tiny genetically engineered insectoids created to destroy mechanoids. Unfortunately they are so small that they can't do much, but they store small amounts of black chitin and black hive insect jelly on their honeycombs.\n\nBee Traits:\n\nProduction time: 24 hours\nNocturnal: No\nPluviophile: No\nUnusual plants needed: None\nMinimum Temperature: -5ºC\nMaximum temperature: 35ºC</description>
							<graphicData>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>   
							<tradeability>Buyable,Sellable</tradeability>
							<comps>			
								<li Class="RimBees.CompProperties_Bees">
									<species>Black</species>
									<comb>RB_Black_Honeycomb</comb>
									<combtimedays>1</combtimedays>
									<nocturnal>false</nocturnal>
									<pluviophile>false</pluviophile>
									<weirdplantneeded>no</weirdplantneeded>
									<tempMin>-5</tempMin>
									<tempMax>35</tempMax>
								</li>   
							</comps>
						</ThingDef>

						<ThingDef ParentName="RB_BlackBeeBase">
							<defName>RB_Bee_Black_Drone</defName>
							<label>black drone</label>
							<graphicData>
								<texPath>Things/Item/Bees/RB_Bee_Black_Drone</texPath>
							</graphicData> 		
						</ThingDef>
						<ThingDef ParentName="RB_BlackBeeBase">
							<defName>RB_Bee_Black_Queen</defName>
							<label>black queen</label>
							<graphicData>
								<texPath>Things/Item/Bees/RB_Bee_Black_Queen</texPath>
							</graphicData> 		
						</ThingDef>

						<!--=============== Cactus ====================-->

						<ThingDef ParentName="RB_BeeBase" Name="RB_CactusBeeBase" Abstract="True" >
							<description>Cactus bees are part animal, part plant. Their honeycombs are semi-edible, producing a weird cactus meat that counts as a vegetable, and medicinal cactipine quills.\n\nBee Traits:\n\nProduction time: 24 hours\nNocturnal: No\nPluviophile: Yes\nUnusual plants needed: none\nMinimum Temperature: 0ºC\nMaximum temperature: 40ºC</description>
							<graphicData>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>   
							<tradeability>Buyable,Sellable</tradeability>
							<comps>			
								<li Class="RimBees.CompProperties_Bees">
									<species>Cactus</species>
									<comb>RB_Cactus_Honeycomb</comb>
									<combtimedays>1</combtimedays>
									<nocturnal>false</nocturnal>
									<pluviophile>true</pluviophile>
									<weirdplantneeded>no</weirdplantneeded>
									<tempMin>0</tempMin>
									<tempMax>40</tempMax>
								</li>   
							</comps>
						</ThingDef>

						<ThingDef ParentName="RB_CactusBeeBase">
							<defName>RB_Bee_Cactus_Drone</defName>
							<label>cactus drone</label>
							<graphicData>
								<texPath>Things/Item/Bees/RB_Bee_Cactus_Drone</texPath>
							</graphicData> 		
						</ThingDef>
