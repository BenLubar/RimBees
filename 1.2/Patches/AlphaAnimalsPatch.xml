							<defName>RB_Bee_Cactus_Queen</defName>
							<label>cactus queen</label>
							<graphicData>
								<texPath>Things/Item/Bees/RB_Bee_Cactus_Queen</texPath>
							</graphicData> 		
						</ThingDef>

						<!--=============== Devil ====================-->

						<ThingDef ParentName="RB_BeeBase" Name="RB_DevilBeeBase" Abstract="True" >
							<description>Devil bees have a piercing gaze that seems to look into your soul. They are expert devilstrand producers, cultivating the strange fungus inside their beehouses. They will need a small amount of devilstrand planted nearby to gather spores.\n\nBee Traits:\n\nProduction time: 72 hours\nNocturnal: No\nPluviophile: No\nUnusual plants needed: Devilstrand\nMinimum Temperature: 0ºC\nMaximum temperature: 30ºC</description>
							<graphicData>
								<graphicClass>Graphic_Single</graphicClass>
							</graphicData>   
							<tradeability>Buyable,Sellable</tradeability>
							<comps>			
								<li Class="RimBees.CompProperties_Bees">
									<species>Devil</species>
									<comb>RB_Devil_Honeycomb</comb>
									<combtimedays>3</combtimedays>
									<nocturnal>false</nocturnal>
									<pluviophile>false</pluviophile>
									<weirdplantneeded>Plant_Devilstrand</weirdplantneeded>
									<tempMin>0</tempMin>
									<tempMax>30</tempMax>
								</li>   
