<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>


			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods> 
				<match Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "PoisonForest"]/wildPlants</xpath>
					<value>
						<RB_Plant_Hive_Jungle>0.01</RB_Plant_Hive_Jungle>
						<RB_Plant_Hive_Swamp>0.01</RB_Plant_Hive_Swamp>
					</value>
				</match>
			</li>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods> 
				<match Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Savanna"]/wildPlants</xpath>
					<value>
						<RB_Plant_Hive_Desert>0.01</RB_Plant_Hive_Desert>
						<RB_Plant_Hive_Arid>0.01</RB_Plant_Hive_Arid>
					</value>
				</match>
			</li>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods> 
				<match Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Volcano"]/wildPlants</xpath>
					<value>
						<RB_Plant_Hive_Arctic>0.01</RB_Plant_Hive_Arctic>
						<RB_Plant_Hive_Icy>0.01</RB_Plant_Hive_Icy>
					</value>
				</match>
			</li>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods> 
				<match Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Wasteland"]/wildPlants</xpath>
					<value>
						<RB_Plant_Hive_Arctic>0.01</RB_Plant_Hive_Arctic>
						<RB_Plant_Hive_Icy>0.01</RB_Plant_Hive_Icy>
					</value>
				</match>
			</li>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Advanced Biomes</li>
				</mods> 
				<match Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "Wetland"]/wildPlants</xpath>
					<value>
						<RB_Plant_Hive_Jungle>0.01</RB_Plant_Hive_Jungle>
						<RB_Plant_Hive_Swamp>0.01</RB_Plant_Hive_Swamp>
					</value>
				</match>
			</li>






		</operations>
	</Operation>


</Patch>

